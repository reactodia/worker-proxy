{
    "compilerOptions": {
        "target": "es2021",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "lib": ["DOM", "ES2021"],
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "useUnknownInCatchVariables": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "noEmit": true,
    }
}
